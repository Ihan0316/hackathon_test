# Copy to .env and adjust values
ENV=dev
TZ=Asia/Seoul

# Ports
SPRING_PORT=8080
AI_PORT=8000
NGINX_PORT=80

# Routing
AI_BASE_URL=http://ai:8000
MAX_BODY=25m

# Mode: fake | real
AI_MODE=fake

# Real mode: .pth classifier
MODEL_PTH_PATH=/app/models/dog_resnet50_120.pth
CLASS_MAP_PATH=/app/models/id2label_120.json
NUM_CLASSES=120

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-miniid2label_120.json

# Similarity (optional)
CLIP_MODEL_NAME=clip-ViT-B-32
INDEX_DIR=/app/index
TOPK=5
